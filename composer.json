{
  "name": "maiksimov/discount",
  "description": "Package for using discounts in Laravel 5",
  "keywords": ["discount", "laravel"],
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Somov",
      "email": "somov.michail.yur@gmail.com",
      "homepage": "https://github.com/maiksimov/discount",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.3"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.4"
  },
  "autoload": {
    "psr-4": {
      "Maiksimov\\Discount\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maiksimov\\Discount\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Maiksimov\\Discount\\DiscountServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}